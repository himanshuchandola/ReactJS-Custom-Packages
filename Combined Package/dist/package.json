{
  "name": "sample-html-package-ss",
  "version": "1.7.9",
  "description": "Sample Html Package",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf rimraf dist",
    "build": "yarn clean && tsc && yarn copy-files && cp package.json README.md ./dist",
