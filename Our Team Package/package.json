    "url": "https://github.com/himanshuchandola"
  },
  "devDependencies": {
    "@types/react": "^17.0.47",
    "copyfiles": "^2.4.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rimraf": "^3.0.2",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "react-router-dom": "^6.3.0",
    "react-scripts": "^5.0.1"
  }
}
